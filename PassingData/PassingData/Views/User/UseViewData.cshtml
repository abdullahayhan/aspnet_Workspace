@using PassingData.Models
@if (ViewData["UserName"].ToString() == "abdullah")
{
    <p>Merhaba @ViewData["UserName"].ToString()</p>
}

else
{
    User user = ViewData["User"] as User;
    <p>Hello Dear @user.FirstName</p>
}

<ul>
    @foreach (User item in ViewData["Users"] as List<User>)
    {
        <li>@item.FirstName</li>
    }
</ul>